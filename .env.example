# Supabase
VITE_SUPABASE_URL="https://YOUR_PROJECT_ID.supabase.co"
VITE_SUPABASE_PUBLISHABLE_KEY="YOUR_SUPABASE_ANON_KEY"

# Google Maps (optional)
VITE_GOOGLE_MAPS_API_KEY="YOUR_GOOGLE_MAPS_API_KEY"
VITE_GOOGLE_MAPS_CLIENT_ID="YOUR_GOOGLE_MAPS_CLIENT_ID"

# Cloudflare Turnstile (captcha)
# Frontend site key (public). Make sure allowed domains include your deployed domain(s), e.g. nowintown.se, www.nowintown.se, localhost
VITE_TURNSTILE_SITE_KEY="YOUR_TURNSTILE_SITE_KEY"
# Backend secret for Supabase function verify-captcha (set in Supabase project env, not in frontend!)
TURNSTILE_SECRET_KEY="YOUR_TURNSTILE_SECRET_KEY"

# Firebase (optional - fill if using Firebase services)
VITE_FIREBASE_API_KEY="YOUR_FIREBASE_API_KEY"
VITE_FIREBASE_AUTH_DOMAIN="your-project.firebaseapp.com"
VITE_FIREBASE_PROJECT_ID="your-project-id"
VITE_FIREBASE_STORAGE_BUCKET="your-project.appspot.com"
VITE_FIREBASE_MESSAGING_SENDER_ID="000000000000"
VITE_FIREBASE_APP_ID="1:000000000000:web:xxxxxxxxxxxxxxxxxxxxxx"
VITE_FIREBASE_MEASUREMENT_ID="G-XXXXXXXXXX"
VITE_FIREBASE_VAPID_KEY="YOUR_FIREBASE_VAPID_KEY"
